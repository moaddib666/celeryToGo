# AWS Credentials
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_REGION=us-east-1

# SQS Configuration
QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/my-queue.fifo

# Application Configuration
ENVIRONMENT=production
LOG_LEVEL=info

# Python Configuration
DJANGO_SETTINGS_MODULE=myproject.settings
PYTHONPATH=/app

# Resource Limits
# These are used by the container runtime
# Adjust based on your workload and available resources
MEMORY_LIMIT=2g
CPU_LIMIT=1.0

# Process Pool Configuration
# Uncomment and adjust if you want to override config.json values
# MIN_PROCESSES=5
# MAX_PROCESSES=30
# MAX_TASKS_PER_PROCESS=100
# MAX_IDLE_TIME=5m
# MAX_PROCESS_AGE=1h

# Observability Configuration
# Uncomment and adjust if you want to override config.json values
# METRICS_ENABLED=true
# METRICS_ENDPOINT=:9090
# TRACING_ENABLED=true
# TRACING_ENDPOINT=jaeger:4317
# SERVICE_NAME=celery-go-worker
# SERVICE_VERSION=0.1.0